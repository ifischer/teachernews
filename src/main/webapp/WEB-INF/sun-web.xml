<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_5-0.dtd">
<sun-web-app error-url="">
  <context-root>/teachernews</context-root>
  
  <!-- Map roles from security realm to groups.
    Groups can than be used in web.xml. -->
  <security-role-mapping>
    <role-name>STUDENT</role-name>
    <group-name>STUDENT</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>TEACHER</role-name>
    <group-name>TEACHER</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>DEANERY</role-name>
    <group-name>DEANERY</group-name>
  </security-role-mapping>
</sun-web-app>
